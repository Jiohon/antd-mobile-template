{
  "name": "antd-mobile-template",
  "version": "1.0.0",
  "description": "基于 antd-mobile 的移动端应用模版",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:check": "eslint . --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,less,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,less,json}\"",
    "type-check": "tsc --noEmit",
    "code-check": "pnpm type-check && pnpm lint:check && pnpm format:check",
    "setup-env": "bash scripts/setup-env.sh",
    "check-env": "tsx scripts/check-env.ts",
    "prepare": "husky"
  },
  "keywords": [
    "react",
    "antd-mobile",
    "mobile",
    "template",
    "typescript",
    "vite"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "alova": "^3.0.9",
    "antd-mobile": "^5.36.1",
    "antd-mobile-icons": "^0.3.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.7.6",
    "lodash": "^4.17.21",
    "rc-field-form": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.20.1",
    "ryt-jssdk": "^1.4.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.4",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^9.1.7",
    "less": "^4.2.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.1.0",
    "terser": "^5.40.0",
    "tsx": "^4.19.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "bash -c 'tsc --noEmit'"
    ],
    "src/**/*.{js,jsx,css,less,json}": [
      "prettier --write"
    ]
  }
}
